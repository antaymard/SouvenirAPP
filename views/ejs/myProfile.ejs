<body>

    <!-- NAVIGATION BAR -->
    <nav class= "header z-depth-0">
      <a href="#" class="brand-logo">Logo</a>
      <div class="nav-wrapper">
        <ul class="right hide-on-med-and-down">
          <li><a href="/" id="lol">HOME</a></li>
          </ul>
        </div>
      </nav>

      <!-- CORPS -->
      <div class="container" style="margin-top : -80px">

        <div class="card horizontal" style='display: flex;'>
              <div class="card-image" style="overflow: hidden;">
                <img id="myProfile_pic" style="height:100%; width:270px; object-fit: cover;" src="<%= userphotoid %>">
                <div class="inputWrapper_btn btn waves-effect waves-light green">
                <form id="fileUp" enctype = "multipart/form-data" action="/myProfile/updateImg" method="post">
                  <label id="addButton" for="userPhoto"><i class="material-icons white-text">publish</i></label>
                  <input type="file" name="userPhoto" id="input_photo" class='inputFilePhoto'/>
                </form>
                </div>
                <!-- <button id="" class="btn-floating waves-effect waves-light green" tabindex="-1"><i class="material-icons">publish</i></button> -->
              </div>
              <div class="card-stacked">
                <div class="card-content row" style='font-size:17px'>
                  <div class='col s3' style='font-weight:300'>
                  <p>Dites quelques</p>
                  <p>Pseudo</p>
                  <p>Nom</p>
                  <P>Prénom</P>
                  <p>Actuellement à</p>
                  <p>Anniversaire</p>
                  <p>Ville de résidence </p>
                  <p>Adresse email</p>
                </div>
                <div class='col m9' style='font-weight:400'>
                  <p>Dites quelques</p>
                  <p><%= username %></p>
                  <p><%= profileNom %></p>
                  <P><%= profilePNom %></P>
                  <p><%= current_city %></p>
                  <p><%= birthday %></p>
                  <p><%= living_city %></p>
                  <p><%= email %></p>
                </div>
                </div>
                <div class="card-action">
                  <button class='right waves-effect waves-red btn-flat'>Modifier</button>
                  <a href="/logout" style="margin-right:10px" class='right waves-effect waves-light btn red'>Fermer la session</a>
                </div>
              </div>
            </div>


        <p class='section_title' style="margin: 30px 0 7px 0">Mes amis</p>
        <div id="myFriends_Space" class="row">



        </div>


        <!-- <p class='section_title'>Mes informations</p> -->
      </div>





  <link rel="stylesheet" href="/styleGeneral.css">
